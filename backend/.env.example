# AMD Ryzen AI 環境變量配置

# ========================================
# AMD 平台配置
# ========================================
# 標記為 AMD Ryzen AI 平台
AMD_RYZEN_AI=true

# ========================================
# Lemonade Server 配置
# ========================================
# Lemonade Server URL
LEMONADE_SERVER_URL=http://localhost:8080

# Lemonade Server API Key（如果需要）
LEMONADE_API_KEY=

# ========================================
# 應用配置
# ========================================
# 後端服務端口
PORT=8001

# 日誌級別
LOG_LEVEL=INFO
